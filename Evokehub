local function adjustSpeed(value)
    local player = game.Players.LocalPlayer
    local character = player.Character
    local humanoid = character and character:FindFirstChild("Humanoid")
    if humanoid then
        local numericValue = tonumber(value)
        if numericValue then
            humanoid.WalkSpeed = numericValue
        end
    end
end
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/UI-Librarys/main/Kavo"))()
local Window = Library.CreateLib("EvokeHub", "DarkTheme")
local Tab = Window:NewTab("funções")
local Section = Tab:NewSection("troll")
local speedTextBox = Section:NewTextBox("Speed", "Digite a velocidade", function(value)
    adjustSpeed(value)
end)
